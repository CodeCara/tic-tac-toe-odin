const gameboard = createGameboard() 

// Example usage:

// empty board
gameboard.board // ["", "", "", "", "", "", "", "", ""]

// set a square
gameboard.set(0, "X")

// get the value of squares
gameboard.get(0) // "X"
gameboard.board //  ["X", "", "", "", "", "", "", "", ""]

// reset
gameboard.reset() // ["", "", "", "", "", "", "", "", ""]